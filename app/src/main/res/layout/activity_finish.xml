<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_finish"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    style="@style/mainMenu"  android:background="#FFE9CE37"
    tools:context="br.com.lucasaquiles.cogg.FinishActivity">



        <View android:id="@+id/emiter_top_left"
            android:layout_gravity="center_horizontal"
            android:layout_alignParentLeft="true"
            android:layout_marginTop="-20dp"
            android:layout_width="1dp"
            android:layout_height="1dp" />

        <View android:id="@+id/emiter_top_right"
            android:layout_gravity="center_horizontal"
            android:layout_alignParentRight="true"
            android:layout_marginTop="-20dp"
            android:layout_width="1dp"
            android:layout_height="1dp" />



<br.com.lucasaquiles.cogg.view.CustomButtton
    android:layout_width="wrap_content" android:id="@+id/back_button"
    android:layout_height="wrap_content" android:text="voltar"
    android:padding="10dp" android:background="@drawable/btn_layout" />
    <RelativeLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerInParent="true"

        >

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/_head_2_ic"
        android:layout_centerHorizontal="true"

        android:id="@+id/imageView11" />



    <br.com.lucasaquiles.cogg.view.CustomTextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Parabééééns!!!!"
        android:textColor="#111"
        android:textSize="30sp"
        android:textAlignment="center"
        android:layout_centerHorizontal="true"
        android:layout_below="@+id/imageView11"
        android:layout_marginTop="10dp"
        android:id="@+id/flag"
        />

    <br.com.lucasaquiles.cogg.view.CustomTextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Parabééééns!!!!"
        android:textColor="#111"
        android:textSize="20sp"
        android:textAlignment="center"
        android:layout_centerHorizontal="true"
        android:layout_below="@+id/flag"
        android:id="@+id/title"

        />

    <br.com.lucasaquiles.cogg.view.CustomTextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Parabééééns!!!!"
        android:textColor="#111"
        android:textSize="20sp"
        android:textAlignment="center"
        android:layout_below="@+id/title"
        android:layout_centerHorizontal="true"
        android:id="@+id/emocao"

        />
    </RelativeLayout>

</RelativeLayout>
